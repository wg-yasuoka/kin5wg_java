<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="sample.mapper.TestMapper">
    <select id="getTest" resultType="sample.entity.Test">
        SELECT
            id,
            name
         FROM
            kin5wgdb.test
         WHERE
            id = #{id}
    </select>
</mapper>